<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.battlejack.ShopController">
   <children>
      <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" spacing="20" styleClass="main-menu" stylesheets="@styles.css">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" styleClass="menu-button" text="2x Boost" onAction="#boost2"/>
            <Button alignment="CENTER" mnemonicParsing="false" styleClass="menu-button" text="3x Boost" onAction="#boost3"/>
            <Button alignment="CENTER" mnemonicParsing="false" styleClass="menu-button" text="Exit" onAction="#exit"/>
         </children>
      </HBox>
   </children>
</AnchorPane>
