<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<AnchorPane stylesheets="@styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.battlejack.ShopController">
   <children>
      <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" spacing="20" styleClass="main-menu">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#boost2" styleClass="menu-button" text="2x Boost" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#boost3" styleClass="menu-button" text="3x Boost" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#exit" styleClass="menu-button" text="Exit" />
         </children>
      </HBox>
      <Text fx:id="winningsText" layoutX="450.0" layoutY="150.0" styleClass="score-label" text="Winnings: $" />
      <Text fx:id="winningsText1" layoutX="330.0" layoutY="350.0" styleClass="score-label" text="\$5000" />
      <Text fx:id="winningsText2" layoutX="540.0" layoutY="350.0" styleClass="score-label" text="\$10000" />
   </children>
</AnchorPane>
